{
    "workbench.startupEditor": "none",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.productIconTheme": "fluent-icons",
    "editor.fontFamily": "CodeNewRoman Nerd Font Bold",
    "codesnap.transparentBackground": true,
    "editor.fontSize": 18,
    "workbench.colorCustomizations": {
    "terminal.foreground": "#ff0000",
    "editor.background": "#000000",
    },
    "code-runner.runInTerminal": true,
    "code-runner.saveAllFilesBeforeRun": true,
    "code-runner.saveFileBeforeRun": true,
    "terminal.integrated.fontSize": 18,
    "terminal.integrated.fontWeightBold": "bold",
    "code-runner.executorMapByGlob": {

        "pom.xml": "cd $dir && mvn clean package"
    },
    "code-runner.executorMap": {

        "javascript": "deno run -A",
        "java": "clear && cd $dir && javac $fileName && java $fileNameWithoutExt",
        "c": "clear && cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "zig": "zig run",
        "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "php": "php",
        "python": "python -u",
        "perl": "perl",
        "perl6": "perl6",
        "ruby": "ruby",
        "go": "go run",
        "lua": "lua",
        "groovy": "groovy",
        "powershell": "powershell -ExecutionPolicy ByPass -File",
        "bat": "cmd /c",
        "shellscript": "bash",
        "fsharp": "fsi",
        "csharp": "scriptcs",
        "vbscript": "cscript //Nologo",
        "typescript": "deno run -A",
        "coffeescript": "coffee",
        "scala": "scala",
        "swift": "swift",
        "julia": "julia",
        "crystal": "crystal",
        "ocaml": "ocaml",
        "r": "Rscript",
        "applescript": "osascript",
        "clojure": "lein exec",
        "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
        "rust": "clear && cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
        "racket": "racket",
        "scheme": "csi -script",
        "ahk": "autohotkey",
        "autoit": "autoit3",
        "dart": "dart",
        "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
        "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
        "haskell": "runghc",
        "nim": "nim compile --verbosity:0 --hints:off --run",
        "lisp": "sbcl --script",
        "kit": "kitc --run",
        "v": "v run",
        "sass": "sass --style expanded",
        "scss": "scss --style expanded",
        "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
        "FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "sml": "cd $dir && sml $fileName",
        "mojo": "mojo run"
    },
    "glassit.alpha": 255,
    "files.autoSave": "afterDelay",
    "redhat.telemetry.enabled": false,
    "explorer.confirmDelete": false,
    "codesnap.containerPadding": "0em",
    "codesnap.boxShadow": "rgba(0, 0, 0, 0.55) 0px 0px 0px",
    "codesnap.showLineNumbers": false,
    "editor.largeFileOptimizations": false,
    "editor.guides.indentation": false,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "editor.renderWhitespace": "none",
    "workbench.editorAssociations": {
        "*.class": "default"
    },
    "window.zoomLevel": 1,
    "sqltools.useNodeRuntime": true,
    "window.titleBarStyle": "custom",
    "todohighlight.isEnable": false,
    "cSpell.userWords": [
        "Cascadia",
        "ciphertext",
        "ciphertxt",
        "codesnap",
        "Diffie",
        "getfact",
        "getsqr",
        "mainpart",
        "MEETMEFIVEPM",
        "plaintxt",
        "publickey",
        "Raghavvram",
        "texthead"
    ],
    "debug.javascript.debugByLinkOptions": "on",
    "liveServer.settings.donotShowInfoMsg": true,
    "extensions.ignoreRecommendations": true,
    "codesnap-plus.containerPadding": "0",
    "codesnap-plus.transparentBackground": true,
    "remote.SSH.remotePlatform": {
        "Dev2": "linux"
    },
    "workbench.sideBar.location": "right",
    "tabnine.experimentalAutoImports": true,
    "explorer.confirmDragAndDrop": false,
    "workbench.activityBar.location": "top",
    "workbench.colorTheme": "Flate",
    "terminal.integrated.fontFamily": "CodeNewRoman Nerd Font ",
    "editor.inlayHints.fontFamily": "CodeNewRoman Nerd Font ",
    "editor.codeLensFontFamily": "CodeNewRoman Nerd Font ",
    "notebook.output.fontFamily": "CodeNewRoman Nerd Font",
    "terminal.integrated.automationProfile.windows": {},
    "security.allowedUNCHosts": [
        "wsl.localhost"
    ],
    "breadcrumbs.enabled": false,
    "editor.linkedEditing": true,
    "window.commandCenter": false,
    "markdown-pdf.breaks": true,
    "markdown-pdf.displayHeaderFooter": false,
    "markdown-pdf.headerTemplate": "<div style=\"font-size: 14px; margin-left: 2cm;\"> <span class='title'></span></div> <div style=\"font-size: 9px; margin-left: auto; margin-right: 1cm; \">%%ISO-DATE%%</div>",
    "markdown-pdf.footerTemplate": "<div style=\"font-size: 14px; margin: 0 auto;\"> <span class='pageNumber'></span> / <span class='totalPages'></span></div>",
    "markdown-pdf.margin.bottom": "2cm",
    "markdown-pdf.margin.left": "2cm",
    "markdown-pdf.margin.right": "2cm",
    "markdown-pdf.margin.top": "2.5cm",
    "markdown-pdf.omitBackground": true,
    "material-icon-theme.activeIconPack": "react"
}